        app:layout_constraintTop_toTopOf="@+id/textView42227" />

    <TextView
        android:id="@+id/textView53227"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nmr"
        android:text="Price"
        android:textColor="@color/gg"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView49227"
        app:layout_constraintEnd_toStartOf="@+id/textView54227"
        app:layout_constraintTop_toTopOf="@+id/textView49227"
