        android:text="Title"
        android:textColor="@color/black"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imageView23227"
        app:layout_constraintStart_toStartOf="@+id/imageView20227"
        app:layout_constraintTop_toTopOf="@+id/imageView19227" />

    <ImageView
        android:id="@+id/imageView21227"
        android:layout_width="0dp"
        android:layout_height="22dp"
        android:layout_marginStart="-8dp"
        android:layout_marginEnd="-8dp"
