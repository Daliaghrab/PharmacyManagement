        android:ems="10"
        android:fontFamily="@font/nmr"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:textColor="@color/gg2"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView23227"
        app:layout_constraintEnd_toEndOf="@+id/imageView23227"
        app:layout_constraintStart_toStartOf="@+id/imageView23227"
        app:layout_constraintTop_toTopOf="@+id/imageView23227" />

    <TextView
        android:id="@+id/textView54227"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
