        android:layout_width="88dp"
        android:layout_height="50dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/rem_from_cart"
        app:layout_constraintBottom_toBottomOf="@+id/imageView23227"
        app:layout_constraintEnd_toEndOf="@+id/imageView19227" />

    <TextView
        android:id="@+id/textView577"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nmr"
        android:text="remove"
        android:textColor="@color/gg"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton127"
        app:layout_constraintEnd_toEndOf="@+id/imageButton127"
        app:layout_constraintStart_toStartOf="@+id/imageButton127"
        app:layout_constraintTop_toTopOf="@+id/imageButton127" />

</androidx.constraintlayout.widget.ConstraintLayout>