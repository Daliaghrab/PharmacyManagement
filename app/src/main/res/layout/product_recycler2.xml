<?xml version="1.0" encoding="utf-8"?>


<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/imageView190022"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/imageView1922"
        android:layout_width="0dp"
        android:layout_height="130dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/eorder_bg" />

    <ImageView
        android:id="@+id/imageView2022"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="8dp"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2322"
        app:layout_constraintStart_toStartOf="@+id/imageView1922"
        app:layout_constraintTop_toTopOf="@+id/imageView2322"
        app:srcCompat="@drawable/border" />

    <TextView
        android:id="@+id/textView4222"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/nmr"
        android:text="Title"
        android:textColor="@color/black"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imageView2322"
        app:layout_constraintStart_toStartOf="@+id/imageView2022"
        app:layout_constraintTop_toTopOf="@+id/imageView1922" />

    <ImageView
        android:id="@+id/imageView2122"
        android:layout_width="0dp"
        android:layout_height="22dp"
        android:layout_marginStart="-8dp"
        android:layout_marginEnd="-8dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView4922"
        app:layout_constraintEnd_toEndOf="@+id/textView4922"
        app:layout_constraintStart_toStartOf="@+id/textView5522"
        app:layout_constraintTop_toTopOf="@+id/textView4922"
        app:srcCompat="@drawable/eorder_status" />

    <ImageView
        android:id="@+id/imageView2322"
        android:layout_width="0dp"
        android:layout_height="80dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView1922"
        app:layout_constraintEnd_toStartOf="@+id/imageButton12"
        app:layout_constraintStart_toEndOf="@+id/imageView2022"
        app:srcCompat="@drawable/customer_prod_desc_bg" />

    <TextView
        android:id="@+id/textView4922"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:fontFamily="@font/nmr"
        android:text="Quantity"
        android:textColor="@color/gg"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView4222"
        app:layout_constraintEnd_toEndOf="@+id/imageView2322"
        app:layout_constraintTop_toTopOf="@+id/textView4222" />

    <TextView
        android:id="@+id/textView5322"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nmr"
        android:text="Price"
        android:textColor="@color/gg"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView4922"
        app:layout_constraintEnd_toStartOf="@+id/textView5422"
        app:layout_constraintTop_toTopOf="@+id/textView4922"
        app:layout_constraintVertical_bias="0.49" />

    <EditText
        android:id="@+id/editTextTextMultiLine22"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="@null"
        android:ems="10"
        android:fontFamily="@font/nmr"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:textColor="@color/gg2"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2322"
        app:layout_constraintEnd_toEndOf="@+id/imageView2322"
        app:layout_constraintStart_toStartOf="@+id/imageView2322"
        app:layout_constraintTop_toTopOf="@+id/imageView2322" />

    <TextView
        android:id="@+id/textView5422"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/nmr"
        android:text=" $"
        android:textColor="@color/gg"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imageView1922"
        app:layout_constraintTop_toTopOf="@+id/textView5322" />

    <TextView
        android:id="@+id/textView5522"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nmr"
        android:text="  "
        android:textColor="@color/gg"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView4922"
        app:layout_constraintEnd_toStartOf="@+id/textView4922"
        app:layout_constraintTop_toTopOf="@+id/textView4922" />

    <ImageButton
        android:id="@+id/imageButton12"
        android:layout_width="88dp"
        android:layout_height="50dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/add_to_cart"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2322"
        app:layout_constraintEnd_toEndOf="@+id/imageView1922" />

    <TextView
        android:id="@+id/textView57"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nmr"
        android:text="Add to cart"
        android:textColor="@color/gg"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton12"
        app:layout_constraintEnd_toEndOf="@+id/imageButton12"
        app:layout_constraintStart_toStartOf="@+id/imageButton12"
        app:layout_constraintTop_toTopOf="@+id/imageButton12" />

</androidx.constraintlayout.widget.ConstraintLayout>